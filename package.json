    "babel-preset-react": "^6.16.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "less": "^2.7.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "nodemon": "^1.11.0",
    "parallelshell": "^2.0.0"
  }
