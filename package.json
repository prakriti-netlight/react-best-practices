  "description": "\"This project is made for codepub meetup\"",
  "main": "index.js",
  "scripts": {
    "develop": "parallelshell 'npm run webpack:watch:dev' 'npm start'",
    "start": "nodemon server.js",
    "less": "lessc --compress frontend/less/all.less > public/build/bundle.css",
